
<div  class="main_title">
  <h2>search result</h2>
  <!-- <p>just like that old recipe book in the kitchen over the frige</p> -->
  <!--  need to modifed this page using bootstrap -->

</div>
<div class="container">
<div class="row">
  <% recipe.forEach((recipe) => {%>
  <div class="col-md-4 col-sm-6" >
        <div class="crane_container">
             <div class="carbox">
                     <a class="img-carbox" >
                        <img src="data:image/jpeg;base64," />
                     </a>
                     <div class="carbox-content">
                         <div class="carbox-title">
                              <a class="title" href="/recipe/<%= recipe._id %>">
                                  <strong><%= recipe.recipe %></strong>
                               </a>
                          </div>
                      <p class="">
                      <%= recipe.recipe %>
                       </p>
                        by: <%= recipe._creator.username %>
                    </div>
                </div>
          </div>
  </div>
  <%  }) %>
</div>
</div>
